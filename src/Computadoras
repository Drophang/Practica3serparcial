public class Computadoras{
    String Marca;
    double memoria;
    String procesador;
    String sistemaOP;
    int precio;
    public Computadoras(String Marca,double memoria,String procesador,String sistemaOP,int precio){
        this.Marca = Marca;
        this.memoria = memoria;
        this.procesador = procesador;
        this.sistemaOP = sistemaOP;
        this.precio = precio;
    }
}